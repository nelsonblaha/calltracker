<%= form_tag sessions_path do %>
        <div class = "field">
                <%= label_tag :netid %>
                <%= text_field_tag :netid  %>
        </div>
        <div class = "field">
                <%= label_tag :password %>
                <%= password_field_tag :password %>
        </div>
        <button class="btn btn-primary" type="submit">Sign in</button>
<% end %>
